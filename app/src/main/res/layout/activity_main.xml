<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#ffffff">


    <Button
        android:id="@+id/button11"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:background="@drawable/rounding"
        android:text="5"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button10" />

    <Button
        android:id="@+id/button3"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_top"
        android:layout_marginStart="@dimen/btn_strtpar"
        android:background="@drawable/rounding"
        android:text="1"
        android:textSize="@dimen/txtsz"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintStart_toStartOf="parent"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
       android:layout_marginStart="@dimen/btn_strtpar"
        android:layout_marginTop="@dimen/btn_toppar"
        android:background="@drawable/rounding"
        android:text="AC"
        android:textColor="@color/Orange"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_top"
        android:layout_marginStart="@dimen/btn_strtpar"
        android:background="@drawable/rounding"
        android:text="7"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:textColor="@color/Orange"
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_toppar"
        android:background="@drawable/rounding"
        android:text="%"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button7"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_toppar"
        android:layout_marginStart="@dimen/btn_strt"
        app:layout_constraintStart_toEndOf="@id/button6"
        android:background="@drawable/rounding"
        android:text="/"
        android:textColor="@color/Orange"
        android:textSize="@dimen/txtsz"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
       android:layout_marginStart="@dimen/btn_strtpar"
        android:layout_marginTop="@dimen/btn_top"
        android:text="4"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        android:background="@drawable/rounding"/>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_toppar"
        android:background="@drawable/rounding"
        app:layout_constraintStart_toEndOf="@id/button2"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/outline_backspace_24"/>

    <Button
        android:id="@+id/button10"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:background="@drawable/rounding"
        android:text="8"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/imageButton" />

    <Button
        android:id="@+id/button12"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:text="2"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/button11"
        android:background="@drawable/rounding"/>

    <Button
        android:id="@+id/button13"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="104dp"
        android:layout_marginTop="@dimen/btn_top"
        android:background="@drawable/rounding"
        android:text="0"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12" />

    <Button
        android:textColor="@color/Orange"
        android:id="@+id/button14"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_top"
        android:layout_marginStart="@dimen/btn_strt"
        android:text="*"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@id/button15"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        android:background="@drawable/rounding"/>

    <Button
        android:id="@+id/button15"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:text="9"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button10"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        android:background="@drawable/rounding"/>

    <Button
        android:id="@+id/button16"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:background="@drawable/rounding"
        android:text="6"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button11"
        app:layout_constraintTop_toBottomOf="@+id/button15" />

    <Button
        android:id="@+id/button17"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:background="@drawable/rounding"
        android:text="3"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button12"
        app:layout_constraintTop_toBottomOf="@+id/button16" />

    <Button
        android:id="@+id/button18"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginLeft="@dimen/btn_strt"
        android:layout_marginTop="@dimen/btn_top"
        android:background="@drawable/rounding"
        android:text="."
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button13"
        app:layout_constraintTop_toBottomOf="@id/button17"/>

    <Button
        android:textColor="@color/Orange"
        android:id="@+id/button19"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_top"
        android:layout_marginStart="@dimen/btn_strt"
        android:background="@drawable/rounding"
        android:text="-"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@id/button16"
        app:layout_constraintTop_toBottomOf="@+id/button14" />

    <Button
        android:textColor="@color/Orange"
        android:id="@+id/button20"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_top"
        android:layout_marginStart="@dimen/btn_strt"
        android:background="@drawable/rounding"
        android:text="+"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toEndOf="@+id/button17"
        app:layout_constraintTop_toBottomOf="@+id/button19" />

    <Button
        android:textColor="@color/Orange"
        android:id="@+id/button21"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginTop="@dimen/btn_top"
        android:layout_marginStart="@dimen/btn_strt"
        android:background="@drawable/rounding"
        android:text="="
        android:textSize="@dimen/txtsz"
        app:layout_constraintTop_toBottomOf="@+id/button20"
        app:layout_constraintStart_toEndOf="@id/button18"/>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginStart="@dimen/btn_strt"
        android:layout_marginEnd="@dimen/btn_end"
        android:layout_marginTop="100dp"
        android:background="@drawable/rounding"
        android:textSize="@dimen/txtsz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>